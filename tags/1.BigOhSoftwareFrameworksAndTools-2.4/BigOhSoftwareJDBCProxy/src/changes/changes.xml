  <properties>
    <title>Project Change History</title>
    <author email="bigohsoftware@gmail.com">Dave W.</author>
  </properties>
  <body>
  	<release version="2.3" date="2010-07-11" description="Moved Project Site">
      <action dev="Dave W." type="update">
        Updated so that maven-generated site is hosted in google code SVN repository.
      </action>
    </release>
  
  	<release version="2.2" date="2010-07-05" description="Move SVN Repository Location">
      <action dev="Dave W." type="update">
        Moved all big-oh.net projects into a Google code SVN repository: https://big-oh.googlecode.com/svn/
      </action>
    </release>
  
  	<release version="2.1" date="2010-06-29" description="Correct Project Domain Name">
      <action dev="Dave W." type="update">
        Updated all java class packages to be rooted under 'net.big_oh' instead of 'com.bigohsoftware'.
      </action>
      <action dev="Dave W." type="update">
        Changed Maven group ID from 'com.bigohsoftware' to 'net.big_oh'.
      </action>
    </release>
  
    <release version="2.0" date="2010-06-08" description="Migrate Project Build to Maven">
      <action dev="Dave W." type="update">
        Updated this project to use a Maven-based build.
      </action>
      <action dev="Dave W." type="remove">
        Removed Eclipse-generate meta-data files from project source control.
      </action>
    </release>

	<release version="1.1" date="2009-08-24" description="Added Support for Framework-Specific .properties File">
      <action dev="Dave W." type="add">
        Support construction of Driver proxy from .properties file.
      </action>
    </release>

    <release version="1.0" date="2009-08-22" description="Initial Project Creation">
      <action dev="Dave W." type="update">
        Created this Big-Oh Software project.
      </action>
    </release>
